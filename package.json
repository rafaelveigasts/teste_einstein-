{
  "name": "grand-line-api",
  "version": "1.0.0",
  "description": "Aplicação Back-End Rol",
  "main": "index.js",
  "repository": "git@github.com:grupoalliar/grand-line-api.git",
  "author": "Alliar Developers",
  "license": "MIT",
  "scripts": {
    "build": "rimraf build && tsc -p tsconfig-build.json",
    "prod": "node --trace-warnings -r dotenv/config build/server.js",
    "dev": "tsnd  --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "dev:stg": "dotenvx run --env-file=.env.stg -- tsnd --inspect --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "dev:prd": "dotenvx run --env-file=.env.prd -- tsnd --inspect --transpile-only --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "test": "vitest",
    "typeorm": "typeorm-ts-node-esm -d ./src/infra/database/typeorm/migration.config.ts",
    "typeorm:sync": "npm run typeorm schema:sync",
    "db:generate": "typeorm-ts-node-esm migration:generate",
    "db:create": "typeorm-ts-node-esm migration:create",
    "db:run": "npx typeorm-ts-node-esm migration:run -d ./src/infra/database/typeorm/migration.config.ts",
    "db:revert": "npx typeorm-ts-node-esm migration:revert -d ./src/infra/database/typeorm/migration.config.ts",
    "db:show": "yarn typeorm migration:show",
    "docker": "docker-compose up -d"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.32.0",
    "@faker-js/faker": "^8.4.1",
    "@types/base-64": "^1.0.2",
    "@types/base64topdf": "^1.1.0",
    "@types/basic-auth": "^1.1.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.14",
    "@types/express-status-monitor": "^1.3.0",
    "@types/form-data": "^2.5.0",
    "@types/ioredis": "^5.0.0",
    "@types/jest": "^29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/mocha": "^10.0.1",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.11.11",
    "@types/recursive-readdir": "^2.2.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1",
    "axios-mock-adapter": "^1.22.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.2.0",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "prettier": "^2.8.0",
    "prisma": "5.18.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^1.4.0"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^7.0.0",
    "@google-cloud/logging-winston": "^5.2.3",
    "@prisma/client": "5.18.0",
    "@types/uuid": "^9.0.1",
    "axios": "^1.2.1",
    "base-64": "^1.0.0",
    "base64topdf": "^1.1.8",
