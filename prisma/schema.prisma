generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Target_audience {
  geeks
  nerds
  minimalistas
  atletas
  esportistas
}

model Survey {
  id              String          @id @default(uuid())
  title           String
  target_audience Target_audience
  star_quantity   Int
  email           String
  created_at      DateTime        @default(now())
  updated_at      DateTime        @updatedAt

  @@map("surveys")
}
